{
  "name": "storybook-assistant",
  "version": "2.0.7",
  "description": "SOTA 2026 Storybook assistant with Vision AI design-to-code, natural language generation, AI-powered accessibility remediation, React Server Components, dark mode generation, and comprehensive testing (Storybook 9, React 19, Next.js 15)",
  "author": {
    "name": "Jesper Vang",
    "url": "https://github.com/flight505"
  },
  "license": "MIT",
  "repository": "https://github.com/flight505/storybook-assistant-plugin",
  "homepage": "https://github.com/flight505/storybook-assistant-plugin",
  "keywords": [
    "storybook",
    "storybook-9",
    "react-19",
    "nextjs-15",
    "server-components",
    "vision-ai",
    "design-to-code",
    "natural-language",
    "ai-powered",
    "accessibility",
    "wcag",
    "dark-mode",
    "component-generation",
    "testing",
    "vitest",
    "playwright",
    "tauri",
    "electron",
    "sota-2026"
  ],
  "commands": "./commands",
  "skills": [
    "./skills/storybook-config",
    "./skills/story-generation",
    "./skills/component-scaffold",
    "./skills/visual-design",
    "./skills/testing-suite",
    "./skills/platform-support",
    "./skills/style-guide-generator",
    "./skills/accessibility-remediation",
    "./skills/server-components",
    "./skills/natural-language-generation",
    "./skills/design-to-code",
    "./skills/dark-mode-generation",
    "./skills/performance-analysis",
    "./skills/ci-cd-generator"
  ],
  "agents": [
    "./agents/accessibility-auditor.md",
    "./agents/component-generator.md"
  ],
  "hooks": ["SessionStart"],
  "requirements": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "optional_dependencies": {
    "openrouter": {
      "env_var": "OPENROUTER_API_KEY",
      "purpose": "AI-powered visual generation (style guides, mockups, diagrams)",
      "required": false
    }
  },
  "claude_api": {
    "uses_claude": true,
    "preferred_env": "CLAUDE_CODE_OAUTH_TOKEN",
    "fallback_env": "ANTHROPIC_API_KEY"
  }
}
